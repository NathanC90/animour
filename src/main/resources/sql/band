drop table band;

create table band (
	id bigSerial primary key,
	name character varying(50),
	member integer
	);

insert into band (id, name, member) values (1, 'Children of Bodom', 5);
insert into band (id, name, member) values (2, 'Bring Me The Horizen', 5);
insert into band (id, name, member) values (3, 'In This Moment', 5);


drop table member;

create table band (
	id bigSerial primary key,
	name character varying(50),
	bandid integer
	);

